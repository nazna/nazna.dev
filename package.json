    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": "next/core-web-vitals",
    "rules": {
      "@next/next/no-img-element": "off"
    },
    "root": true
  },
  "dependencies": {
    "clsx": "1.2.1",
    "next": "12.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.6.4",
    "@types/react": "18.0.15",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.0.1",
    "prettier-plugin-style-order": "0.2.2",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": "18.x",
    "npm": "8.x"
  }
}
