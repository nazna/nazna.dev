{
  "name": "nazna.dev",
  "private": true,
  "type": "module",
  "version": "2.0.0",
  "description": "A website about nazna",
  "author": "nazna <naznagg@gmail.com> (https://nazna.dev)",
  "scripts": {
    "dev": "npm run build && npm run start",
    "build": "ts-node ./src/main.mts",
    "start": "wrangler pages dev ./dist",
    "publish": "wrangler pages publish ./dist",
    "lint": "npm run typecheck && npm run eslint && npm run prettier",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint --ignore-path .gitignore --no-error-on-unmatched-pattern --fix .",
    "prettier": "prettier --ignore-path .gitignore --list-different --write ."
  },
  "dependencies": {
    "@markdoc/markdoc": "0.1.13",
    "feed": "4.2.2",
    "image-size": "1.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "wrangler": "2.1.13",
    "yaml": "2.1.3"
  },
  "devDependencies": {
    "@swc/core": "1.3.11",
    "@types/node": "18.11.8",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "prettier-plugin-css-order": "1.3.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": "19.x",
    "npm": "8.x"
  }
}
