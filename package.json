  "dependencies": {
    "clsx": "1.1.1",
    "next": "12.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@types/node": "17.0.27",
    "@types/react": "18.0.6",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "prettier": "2.6.2",
    "prettier-plugin-organize-imports": "2.3.4",
    "prettier-plugin-style-order": "0.2.2",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": "17.x",
    "npm": "8.x"
  }
}
