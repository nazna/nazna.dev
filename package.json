  "description": "A website about nazna",
  "license": "UNLICENSED",
  "author": "nazna <naznagg@gmail.com> (https://github.com/nazna)",
  "scripts": {
    "build": "next build && next export",
    "dev": "next dev",
    "eslint": "eslint --fix .",
    "format": "prettier --ignore-path .gitignore --write .",
