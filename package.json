{
  "name": "nazna.dev",
  "private": true,
  "type": "module",
  "version": "2.1.0",
  "description": "A website about nazna",
  "author": "nazna <naznagg@gmail.com> (https://nazna.dev)",
  "scripts": {
    "dev": "NODE_ENV=development npm run build && npm run start",
    "build": "ts-node ./src/main.mts",
    "start": "wrangler pages dev ./dist --r2=BUCKET --compatibility-date=$(date '+%Y-%m-%d')",
    "publish": "npm run build && wrangler pages publish ./dist",
    "lint": "npm run typecheck && npm run eslint && npm run prettier",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint --ignore-path .gitignore --no-error-on-unmatched-pattern --fix .",
    "prettier": "prettier --ignore-path .gitignore --list-different --write ."
  },
  "dependencies": {
    "@markdoc/markdoc": "0.2.1",
    "feed": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "wrangler": "2.4.4",
    "yaml": "2.1.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20221111.1",
    "@swc/core": "1.3.20",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "postcss": "8.4.19",
    "prettier": "2.8.0",
    "prettier-plugin-css-order": "1.3.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "engines": {
    "node": "19.x",
    "npm": "9.x"
  }
}
